<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".MainActivity">

       <Button
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text="Inserir alunos"
               android:onClick="clicar"
               android:id="@+id/btnInserirAlunos"/>

    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Listar alunos"
            android:onClick="clicar"
            android:id="@+id/btnListarAlunos"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textViewResultadoAlunos"
            android:text="Resultado"/>

    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Inserir disciplina"
            android:id="@+id/btnInserirDisciplina"/>

    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Listar disciplina"
            android:id="@+id/btnListarDisciplina"/>

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textViewResultadoDisciplinas"
            android:text="Resultado"/>


</LinearLayout>